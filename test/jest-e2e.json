{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@lib/providers/(.*)": "<rootDir>/../libs/providers/src/$1",
    "@lib/providers": "<rootDir>/../libs/providers/src",
    "@lib/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@lib/entities": "<rootDir>/../libs/entities/src",
    "@lib/post/(.*)": "<rootDir>/../libs/post/src/$1",
    "@lib/post": "<rootDir>/../libs/post/src",
    "@lib/errors/(.*)": "<rootDir>/../libs/errors/src/$1",
    "@lib/errors": "<rootDir>/../libs/errors/src"
  }
}